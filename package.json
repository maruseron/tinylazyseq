{
  "name": "tinylazyseq",
  "type": "module",
  "version": "0.4.0",
  "description": "Tiny generator-based lazy sequence implementation. Loosely inspired by the Kotlin Sequences implementation and the JavaScript Array API.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js && node node_modules/istanbul-badges-readme/lib/index.js -- --logo=jest",
    "build": "tsc && npx typedoc ./index.ts --readme ./readme.md --out docs/"
  },
  "keywords": [
    "lazy",
    "sequence",
    "generators",
    "iterable"
  ],
  "author": "maruseron",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/maruseron/tinylazyseq"
  },
  "homepage": "https://github.com/maruseron/tinylazyseq#readme",
  "bugs": {
    "url": "https://github.com/maruseron/tinylazyseq/issues",
    "email": "marcelorodfu@gmail.com"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "istanbul-badges-readme": "^1.7.0",
    "jest": "^27.3.1",
    "typedoc": "^0.22.7",
    "typedoc-theme-dark": "^0.0.5"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "json-summary",
      "text"
    ]
  }
}
